{
  "npmClient": "npm",
  "private": true,
  "version": "independent",
  "hoist": true,
  "stream": true,
  "bootstrap": {
    "npmClientArgs": ["--no-package-lock"]
  },
  "command": {
    "publish": {
      "allowBranch": ["master", "feature/*"],
      "registry": "https://registry.npmjs.org/"
    }
  },
  "useNx": false
}
